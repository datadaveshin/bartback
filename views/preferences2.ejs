<!DOCTYPE html>
<html lang="en">


<head>
    <!-- ###### META ####### -->
    <meta charset="UTF-8">
    <!-- <meta http-equiv="X-UA-Compatible" content="ie=edge"> -->

    <!-- ## Materialize - Let browser know website is optimized for mobile ## -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <!-- ###### FONTS ####### -->
    <!--Import Google Icon Font-->
    <link href="http://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">


    <!-- ###### CSS ####### -->
    <link type="text/css" rel="stylesheet" href="/css/materialize.min.css" media="screen,projection" />
    <!-- <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.7/css/materialize.min.css"  media="screen,projection"> -->
    <link rel="stylesheet" href="css/master.css" media="screen" title="no title">


    <!-- ###### JQUERY AND MATERIALIZE JS ####### -->
    <!-- jQuery JS - precede and use 2.1.1 version when using Materialize -->
    <!-- <script type="text/javascript" src="https://code.jquery.com/jquery-2.1.1.min.js"></script> -->
    <!-- <script type="text/javascript" src="/js/jquery-2.1.1.min.js"></script> -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.0/jquery.min.js"></script>
    <script type="text/javascript" src="/js/materialize.min.js"></script>

    <!-- ###### TITLE ####### -->
    <title>Bart To The Future</title>
</head>


<body>
    <!-- ###### NAV BAR ####### -->
    <nav>
        <div class="container">
            <div class="row">
                <div class="nav-wrapper col s12">
                    <a href="#" class="brand-logo">BARTBack Preferences</a>
                    <a href="#" data-activates="menu" class="button-collapse"><i class="material-icons">menu</i></a>


                    <ul class="right hide-on-med-and-down nav-injector">
                        <!-- <li class="active"><a href="/auth/login" class="white-text">Log In</a></li> -->
                        <li><a href="/auth/login" class="white-text">Log In</a></li>
                        <li><a href='/register' class="white-text">Register</a></li>
                    </ul>

                    <ul class="side-nav nav-injector2" id="menu">
                        <!-- <li class="active"><a  href='/auth/login'>Log In</a></li> -->
                        <li><a href='/auth/login'>Log In</a></li>
                        <li><a href='/register'>Register</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </nav>


    <!-- ###### MAIN ####### -->
    <main>
        <div class="container">
            <!-- <h5 class="loggedIn">Hi <%= typeof userName!= "undefined" ? userName : "noname" %>!</h5> -->
            <input type="hidden" name="" id="loggedInToo" value="<%= typeof userName!= "undefined" ? userName : "noname" %>">
            <h5 class="loggedIn"></h5>

            <div class="row">
                <section id="point0"></section>
            </div>

            <div class="row">
                <section id="point1"></section>
            </div>

            <div class="row">
                <section id="point2"></section>
            </div>

            <div class="row">
                <section id="point3"></section>
            </div>

            <div class="row">
                <section id="point4"></section>
            </div>
    
            <div class="form-container">
                <form class="" action="/preferences" method="post">
                    <div class="input-field col s12">
                        <select class="" name="home">
                            <option value="" disabled selected>Choose your option</option>
                            <option value="12th">12th St. Oakland City Center</option>
                            <option value="16th">16th St. Mission (SF)</option>
                            <option value="19th">19th St. Oakland</option>
                            <option value="24th">24th St. Mission (SF)</option>
                            <option value="ashb">Ashby (Berkeley)</option>
                            <option value="balb">Balboa Park (SF)</option>
                            <option value="bayf">Bay Fair (San Leandro)</option>
                            <option value="cast">Castro Valley</option>
                            <option value="civc">Civic Center (SF)</option>
                        </select>
                        <label>Home Station</label>
                    </div>
                    <div class="input-field col s12">
                        <select class="" name="away">
                            <option value="" disabled selected>Choose your option</option>
                            <option value="12th">12th St. Oakland City Center</option>
                            <option value="16th">16th St. Mission (SF)</option>
                            <option value="19th">19th St. Oakland</option>
                            <option value="24th">24th St. Mission (SF)</option>
                            <option value="ashb">Ashby (Berkeley)</option>
                            <option value="balb">Balboa Park (SF)</option>
                            <option value="bayf">Bay Fair (San Leandro)</option>
                            <option value="cast">Castro Valley</option>
                            <option value="civc">Civic Center (SF)</option>
                        </select>
                        <label>Away Station</label>
                    </div>
                    <div class="input-field col s12">
                        <button class="btn waves-effect waves-light" type="submit" name="action">Submit
                        <i class="material-icons right">send</i>
                        </button>
                    </div>
                </form>
            </div>
        </div>
    </main>

    <!-- ###### FOOTER ####### -->
    <%- include("footer") %>

    <!-- ###### JAVASCRIPT SCRIPTS ####### -->
    <script type="text/javascript" src="js/helperFunctions-1.js"></script>
    <script type="text/javascript" src="js/data.js"></script>
    <script type="text/javascript" src="js/preferences2.js"></script>
</body>

</html>
